# This file is a placeholder for environment variables required to run this project.
# Copy this file to .env.local and replace placeholder values with your actual credentials.

# Password for protected pages/routes
PAGE_ACCESS_PASSWORD=your_password_here

# Resend Configuration
# Email service for contact form notifications
RESEND_API_KEY=your_resend_api_key_here

# Listmonk Configuration
# Your Listmonk instance URL and API credentials
NEXT_PUBLIC_LISTMONK_URL=https://your-listmonk-domain.com
LISTMONK_BASE_URL=https://your-listmonk-domain.com
LISTMONK_API_URL=https://your-listmonk-domain.com/api
LISTMONK_USERNAME=your_api_username
LISTMONK_PASSWORD=your_api_token_here

# List IDs (UUIDs for client-side)
NEXT_PUBLIC_CONTACT_LIST_ID=your_contact_list_uuid
NEXT_PUBLIC_NEWSLETTER_LIST_ID=your_newsletter_list_uuid

# List ID Mappings (Integers for Listmonk API)
CONTACT_LIST_INT_ID=1
NEWSLETTER_LIST_INT_ID=2

# Notification Email
NOTIFICATION_EMAIL=your_email@example.com
